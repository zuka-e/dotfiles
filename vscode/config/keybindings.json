[
  //=========================================================
  // Disable default
  //=========================================================
  // - System
  {
    "key": "f1",
    "command": "-workbench.action.showCommands"
  },
  {
    "key": "ctrl+w",
    "command": "-workbench.action.switchWindow"
  },
  {
    "key": "shift+cmd+c",
    "command": "-workbench.action.terminal.openNativeConsole",
    "when": "!terminalFocus"
  },
  // - Extensions
  {
    "key": "cmd+m",
    "command": "-markdown.extension.editing.toggleMath",
    "when": "editorLangId == 'markdown'"
  },
  //=========================================================
  // Enable custom (maybe disable default)
  //=========================================================
  {
    "key": "cmd+n",
    "command": "workbench.files.action.createFileFromExplorer"
  },
  {
    "key": "cmd+shift+n",
    "command": "workbench.files.action.createFolderFromExplorer"
  },
  {
    "key": "cmd+t",
    "command": "workbench.action.files.newUntitledFile"
  },
  {
    "key": "cmd+[",
    "command": "workbench.action.navigateBack",
    "when": "!terminalFocus && canNavigateBack"
  },
  {
    "key": "cmd+]",
    "command": "workbench.action.navigateForward",
    "when": "!terminalFocus && canNavigateForward"
  },
  {
    "key": "ctrl+r",
    "command": "editor.action.previousMatchFindAction",
    "when": "findWidgetVisible"
  },
  {
    "key": "ctrl+s",
    "command": "editor.action.nextMatchFindAction",
    "when": "findWidgetVisible"
  },
  // Note:
  // - Maybe overwritten by keyboard customizer like Karabiner
  // - Text manipulation commands won't work in search fields
  {
    "key": "ctrl+w",
    "command": "deleteWordPartLeft",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+shift+w",
    "command": "deleteWordPartRight",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+u",
    "command": "deleteAllLeft",
    "when": "textInputFocus && !editorReadonly"
  }
]
